{
  "kind": "collectionType",
  "collectionName": "resumes",
  "info": {
    "singularName": "resume",
    "pluralName": "resumes",
    "displayName": "Resume"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "candidate": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::candidate-profile.candidate-profile",
      "mappedBy": "resume"
    },
    "resumeFile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "parsedData": {
      "type": "json"
    },
    "resumeStatus": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Archived"
      ]
    },
    "visibility": {
      "type": "enumeration",
      "enum": [
        "Public",
        "Private"
      ]
    }
  }
}
